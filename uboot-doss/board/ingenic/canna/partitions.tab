property:
    disk_size = 2048m
    gpt_header_lba = 512
    custom_signature = 0

partition:
	#name     =  start,     size,  fstype
	uboot     =     0m,       1m,  EMPTY
	nv        =     1m,       1m,  EMPTY
	kernel    =     2m,       3m,  EMPTY
	updaterfs =     5m,      50m,  LINUX_FS
	appfs     =    55m,     200m,  LINUX_FS
	ota       =    255m,    100m,  LINUX_FS
	reserve   =    355m,   1693m,  LINUX_FS

#fstype could be: LINUX_FS, FAT_FS, EMPTY
